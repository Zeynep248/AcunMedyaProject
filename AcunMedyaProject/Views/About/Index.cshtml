@model List<AcunMedyaProject.Models.Tbl_About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Burası Hakkımda Sayfası </h2>



<div class=" container">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>AboutID</th>
                <th>Resim</th>
                <th>Başlık</th>
                <th>Doğum Günü</th>
                <th>Web Sitesi</th>
                <th>Numara</th>
                <th>Şehir</th>
                <th>Yaş</th>
                <th>E mail</th>
                <th>Kategori İsmi</th>
                <th>Açıklama 1</th>
                <th>Açıklama 2</th>
                <th>Derece</th>
                <th>Güncelle</th>
                <th>Sil</th>
            <tr>
        </thead>
        <tbody>
            <tr>


                @foreach (var item in Model)
                {
                    <td>@item.AboutID</td>
                    <td>@item.ImageUrl</td>
                    <td>@item.Title</td>
                    <td>@item.BirthDay</td>
                    <td>@item.WebSite</td>
                    <td>@item.Phone</td>
                    <td>@item.City</td>
                    <td>@item.Age</td>
                    <td>@item.Email</td>
                    <td>@item.FreeLance</td>
                    <td>@item.Description1</td>
                    <td>@item.Description2</td>
                    <td>@item.Degree</td>
                    <td><a href="#" class="btn btn-success">Güncelle</a></td>
                    <td><a href="/About/DeleteAbout/@item.AboutID" class="btn btn-dark">Sil</a></td>

                }


        </tbody>
    </table>
    <a href="/About/CreateAbout/" class="btn btn-success">Oluştur</a>

</div>
